(select sum(dCantidad)  from bitacoradepernocta b31 where b31.sNumeroOrden = ba.sNumeroOrden and b31.dIdFecha=concat( $P{iAnio},"-",$P{iMes},"-", "31") and b31.sIdCuenta = b.sIdCuenta and b31.sContrato = b.scontrato) as Dia31
