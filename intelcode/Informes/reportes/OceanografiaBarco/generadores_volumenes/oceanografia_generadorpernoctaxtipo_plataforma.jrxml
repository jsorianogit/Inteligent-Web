(select sum(dCantidad)  from bitacoradepernocta b27 where b27.sNumeroOrden = ba.sNumeroOrden and b27.dIdFecha=concat( $P{iAnio},"-",$P{iMes},"-", "27") and b27.sIdCuenta = b.sIdCuenta and b27.sContrato = b.scontrato) as Dia27,
(select sum(dCantidad)  from bitacoradepernocta b28 where b28.sNumeroOrden = ba.sNumeroOrden and b28.dIdFecha=concat( $P{iAnio},"-",$P{iMes},"-", "28") and b28.sIdCuenta = b.sIdCuenta and b28.sContrato = b.scontrato) as Dia28,
