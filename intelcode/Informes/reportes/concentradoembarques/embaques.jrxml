	<parameter name="sContrato" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["428237843"]]></defaultValueExpression>
	</parameter>
	<parameter name="sIdConvenio" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="dIdFecha" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["2007-12-13"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
	@Actividad:=sNumeroActividad as sNumeroActividad,
	mDescripcion,
	sMedida,
	@dCantidadAnexo:=dCantidadAnexo as dCantidadAnexo,
	dPonderado,
	dVentaMN,
  #cantidad generada
	@dGenerado:=(
		Select Sum(e.dCantidad) as dGenerado
		From estimacionxpartida e INNER JOIN estimaciones e2 ON
    		(
			 	e.sContrato = e2.sContrato
				 And e.sNumeroOrden = e2.sNumeroOrden
				 And e.sNumeroGenerador = e2.sNumeroGenerador
				 And e2.dFechaFinal <= $P{dIdFecha}
			)
    	where
		 e.sContrato = $P{sContrato}
		 And e.sNumeroActividad = @Actividad
		 And e.lEstima = 'Si'
		Group By e.sNumeroActividad
	) as dGenerado,

  #cantidad suministrada
 	@dSuministrado:=(
		Select SUM(e.dCantidad) as dSuministrado
		From anexo_psuministro e
		INNER JOIN anexo_suministro e2 ON
	         	(e.sContrato = e2.sContrato
			And e.iFolio = e2.iFolio
			And e2.dFechaAviso <=  $P{dIdFecha})
	         Where
			e.sContrato =  $P{sContrato}
			And e.sNumeroActividad = @Actividad
		Group By e.sNumeroActividad)  as dSuministrado,

  #si la cantidad generada es null hacerla cero
  if(@dGenerado is null,@dGenerado:=0,@dGenerado:=@dGenerado+0) as doGenerado,

  #si la suministrada es null hacerla cero
  if(@dSuministrado is null,@dSuministrado:=0,@dSuministrado:=@dSuministrado+0)  as doSuministrado,

  #monto de la cantidad generada
	(@dGenerado*dVentaMN) as dMontoDGenerado,

  #por suministrar
  if((@dGenerado+0)>(@dSuministrado+0),@dGenerado-@dSuministrado,0) as dPsuministrar,

  #por reportar
  if((@dCantidadAnexo+0)>(@dSuministrado+0),@dCantidadAnexo-@dSuministrado,0) as dPreportar,

  #cantidad por generar
  @dPgenerar:= if((@dGenerado+0)>(@dSuministrado+0),0,@dSuministrado-@dGenerado) as dPorGenerar,

  #monto de la cantidad por generar
  (@dPgenerar*dVentaMN) as dMontoPgenerar

From
	actividadesxanexo
where
	sContrato = $P{sContrato}
	And sIdConvenio =$P{sIdConvenio}
	and sTipoActividad = 'Actividad'
Order By iItemOrden]]></queryString>

