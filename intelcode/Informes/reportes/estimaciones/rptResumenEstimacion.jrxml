	<queryString><![CDATA[select a.mDescripcion, a.dVentaMN, a.dMontoMN, a.dMontoAcumuladoMN,
@creo:=e.sIdUsuario, @valido:=e.sIdUsuarioValida, @autorizo:=e.sIdUsuarioAutoriza,e.lStatus,
(select u.sNombre from usuarios u where sIdUsuario = @creo) as ucreo, 
(select u.sPuesto from usuarios u where sIdUsuario = @creo) as pcreo,
(select u.sNombre from usuarios u where sIdUsuario = @valido) as uvalido,
(select u.sPuesto from usuarios u where sIdUsuario = @valido) as pvalido,
(select u.sNombre from usuarios u where sIdUsuario = @autorizo) as uautorizo,
(select u.sPuesto from usuarios u where sIdUsuario = @autorizo) as pautorizo,
o.sElementoPEP, o.sFondo, o.sPosicionFinanciera, o.sCuentaMayor,
o.sCentroCosto, o.sCentroGestor, o.sCentroBeneficio,
p.dFechaInicioContrato, p.dFechaFinalContrato,
(select sum(dMontoMN) from actividadesxestimacion where  sContrato= $P{sContrato} and iNumeroEstimacion = $P{iNumeroEstimacion} and sTipoActividad = $P{sTipoActividad} group by sContrato) as monto,
(select sum(dMontoAcumuladoMN) from actividadesxestimacion where  sContrato= $P{sContrato} and iNumeroEstimacion = $P{iNumeroEstimacion} and sTipoActividad = $P{sTipoActividad} group by sContrato) as acumulado
from actividadesxestimacion a
inner join contratos o
on (o.sContrato = a.sContrato)
inner join estimaciones e
on (    e.sContrato   	    = a.sContrato
    and e.sNumeroOrden 	    = $P{sNumeroOrden}
    and e.iNumeroEstimacion = a.iNumeroEstimacion)
inner join estimacionperiodo p
on (	p.sContrato 	    = a.sContrato
    and p.iNumeroEstimacion = a.iNumeroEstimacion)
where 	  a.sContrato 	    = $P{sContrato}      
      and a.iNumeroEstimacion = $P{iNumeroEstimacion}
      and a.sTipoActividad    = $P{sTipoActividad}]]></queryString>

