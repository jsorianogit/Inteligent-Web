	<queryString><![CDATA[select
      p.sDescripcion,
      p.sMedida,
      p.dVentaMN,
      bp.dCantidad,
      (p.dVentaMN*bp.dCantidad) as dTotalMN
from bitacoradepersonal bp
inner join bitacoradeactividades ba
on(bp.sContrato = ba.sContrato and bp.dIdFecha=ba.dIdFecha and bp.iIdDiario=ba.iIdDiario)
inner join personal p
on(p.sIdPersonal=bp.sIdPersonal)
where ba.sContrato=$P{sContrato}
and ba.sIdTurno=$P{sIdTurno}
and ba.dIdFecha=$P{dIdFecha}
group by p.sIdPersonal
order by bp.sDescripcion]]></queryString>

