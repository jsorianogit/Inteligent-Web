                           date_format(ETP.dFechaFinal,'%d/%m/%Y') as F_Final,
                           TPE.sDescripcion as Tipo,
                           concat('$ ', format(ETP.dMontoMN,2))  as Monto_MN,
                           concat('$ ', format(ETP.dRetencionMN,2))  as Retencion_MN,
