                              INSU.sIdGrupo as Familia,
                              '' as Cometarios,
                              PPE.dCosto as NCosto,
                              PPE.iFolioPedido as FolioPedido
                           from
                              anexo_ppedido PPE
                           inner join
                              anexo_pedidos PE
                           on (PE.sContrato = '425027814'
                               and PE.iFolioPedido ='0' )
                           inner join
                              insumos INSU
                           on (INSU.sContrato = '425027814'
                               and INSU.sIdInsumo = PPE.sIdInsumo )
                           where
                              PPE.sContrato='425027814'
                              and PPE.iFolioPedido = '0'
                              and PPE.lStatus = 'Pendiente'
                              or  PPE.lStatus = 'Incompleto'   ")
