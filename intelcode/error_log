                           inner join
                              anexo_pedidos PE
                           on (PE.sContrato = 'OT-02'
                               and PE.iFolioPedido ='0' )
                           inner join
                              insumos INSU
                           on (INSU.sContrato = 'OT-02'
                               and INSU.sIdInsumo = PPE.sIdInsumo )
                           where
                              PPE.sContrato='OT-02'
                              and PPE.iFolioPedido = '0'
                              and PPE.lStatus = 'Pendiente'
                              or  PPE.lStatus = 'Incompleto'   ")
